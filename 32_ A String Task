#include<iostream>
using namespace std;
int main()
{
	string str;
	cin>>str;
	for (int i = 0; i < str.size(); i++) {
		str[i] = tolower(str[i]);
		if (str[i] == 'a' || str[i] == 'i' || str[i] == 'o' || str[i] == 'u' || str[i] == 'e' || str[i] == 'y')
			continue;
		cout <<"."<< str[i];
	}
	return 0;
}
