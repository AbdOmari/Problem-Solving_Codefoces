#include <iostream>
#include <string>

using namespace std;

int main() {
	string s;
	cin >> s;

	string hello = "hello";
	int o = 0;
	for (int i = 0; i < s.length(); i++) {
		if (s[i]==hello[o]) {
			o++;
		}
	}

	if (o ==5) {
		cout << "YES" << endl;
	}
	else {
		cout << "NO" << endl;
	}

	return 0;
}
